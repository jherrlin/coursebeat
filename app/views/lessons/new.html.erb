<h1>New Lesson</h1>
<br>
<form action="/courses/<%= params[:course_id] %>/lessons" method="post">
	<input type="text" value="Title" class="title" name="title" class="center"><br><br>

	<div id="keyConceptHolder"></div>

	<input type="button" class="btn btn-primary btn-sm" value="Add Key Concept" onClick="addKeyConcept()"><br>
	<br><input type="submit" class="btn btn-danger btn-lg" data-course=<%= params[:course_id] %> id="submitButton" value="Submit">
</form>