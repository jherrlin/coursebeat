<h1>Create a Course</h1><br>

<%= form_for @course do |c| %>
  <%= c.text_field :course_name, placeholder: "Course name" %> 
  <input type="button" class="btn btn-primary btn-md" value="Add Instructor" onClick="addInstructor()"><br>
  <div id="instructorHolder"></div><br>
  <%= c.submit 'Create Course', class: 'btn btn-success btn-lg' %>
<% end %>

